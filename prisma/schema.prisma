generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model User {
  name         String
  email        String       @id
  phone        String?
  profilePhoto String?
  createdAt    DateTime     @default(now())
  updatedAt    DateTime     @updatedAt
  stripePaid   Boolean      @default(false)
  clientrate   clientrate[]
}

